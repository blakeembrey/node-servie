{
  "name": "servie",
  "version": "3.2.4",
  "description": "Standard HTTP interfaces",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "LICENSE",
    "logo.svg"
  ],
  "browser": {
    "./dist/body/node/index.js": "./dist/body/browser/index.js"
  },
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" --project tsconfig.json",
    "build": "rm -rf dist/ && tsc",
    "specs": "jest --coverage",
    "test": "npm run lint && npm run build && npm run specs && npm run bundle && npm run bundle:size",
    "bundle": "browserify . -o browser.js",
    "bundle:size": "du -h browser.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/serviejs/servie.git"
  },
  "keywords": [
    "request",
    "response",
    "http",
    "interface",
    "req",
    "res",
    "server"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/serviejs/servie/issues"
  },
  "homepage": "https://github.com/serviejs/servie",
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "transform": {
      "\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "browserify": "^16.2.3",
    "jest": "^24.6.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.1",
    "tslint": "^5.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "byte-length": "^1.0.2"
  }
}
